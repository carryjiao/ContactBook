<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="80dp"
    android:gravity="center_vertical"
    android:orientation="vertical">

    <TextView
        android:id="@+id/tv_contact_personal_show_email_type"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginLeft="@dimen/record_item_in_detail_margin_left"
        android:text="个人"
        android:textSize="20sp"
        android:typeface="monospace"
        android:textColor="@color/colorCharBlue"/>

    <TextView
        android:id="@+id/tv_contact_personal_show_email"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="5dp"
        android:layout_marginLeft="@dimen/record_item_in_detail_margin_left"
        android:text="465938001@qq.com"
        android:textSize="20sp"
        android:typeface="monospace"
        android:textColor="@color/colorCharBlue"/>

</LinearLayout>
